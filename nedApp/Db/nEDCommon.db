record(bi, "$(P)nED:Running")
{
    field(DESC, "Available when nED is running")
    field(ZNAM, "Not running")
    field(ONAM, "Running")
    field(VAL,  "1")
    field(PINI, "YES")
}
record(stringin, "$(P)nED:Version")
{
    field(DESC, "nED Version")
    field(VAL,  "1.8.x")
    field(PINI, "YES")
}
record(dfanout, "$(P)nED:PreStart")
{
    field(VAL,  "1")
    # A placeholder record that can be used by beamlines
    # specific records
}
