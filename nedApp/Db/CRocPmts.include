record(calc, "$(P)PmtsCheck1")
{
    field(INPA, "$(P)Y1:Pmt:Alarm NPP")
    field(INPB, "$(P)Y2:Pmt:Alarm NPP")
    field(INPC, "$(P)Y3:Pmt:Alarm NPP")
    field(INPD, "$(P)Y4:Pmt:Alarm NPP")
    field(INPE, "$(P)Y5:Pmt:Alarm NPP")
    field(INPF, "$(P)Y6:Pmt:Alarm NPP")
    field(INPG, "$(P)Y7:Pmt:Alarm NPP")
    field(INPH, "$(P)X1:Pmt:Alarm NPP")
    field(INPI, "$(P)X2:Pmt:Alarm NPP")
    field(INPJ, "$(P)X3:Pmt:Alarm NPP")
    field(INPK, "$(P)X4:Pmt:Alarm NPP")
    field(INPL, "$(P)X5:Pmt:Alarm NPP")
    field(CALC, "A+B+C+D+E+F+G+H+I+J+K+L")
}
