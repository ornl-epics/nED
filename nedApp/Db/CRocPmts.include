# Number of PMTs in alarm state
record(calc, "$(P)NumFailedPmts")
{
    field(INPA, "$(P)PmtsCheck1 PP")
    field(INPB, "$(P)PmtsCheck2 PP")
    field(INPC, "$(P)PmtsCheck3 PP")
    field(CALC, "A+B+C")
}

