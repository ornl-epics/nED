record(calc, "$(P)HwStatus")
{
    field(INPA, "$(P)Configured.SEVR NPP NMS")
    field(INPB, "$(P)Discovered.SEVR NPP NMS")
    field(INPC, "$(P)TempBoard.SEVR NPP NMS")
    field(CALC, "A#2&&B=0&&C#2")
    field(LOW,  "0")
    field(LSV,  "MAJOR")
}
record(mbbo, "$(P)DiagEn")
{
    field(ZRST, "disable")
    field(ONST, "enable")
    field(ZRVL, "1")
    field(ONVL, "0")
    field(DTYP, "Raw Soft Channel")
    field(OUT,  "$(P)OutputMode PP")
}
