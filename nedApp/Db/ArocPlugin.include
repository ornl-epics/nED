record(calc, "$(P)HwStatus")
{
    field(INPA, "$(P)Configured.SEVR NPP")
    field(INPB, "$(P)Discovered.SEVR NPP")
    field(INPC, "$(P)TempBoard.SEVR NPP")
    field(CALC, "(A+B+C)=0")
    field(LOW,  "0")
    field(LSV,  "MAJOR")
}
record(mbbo, "$(P)DiagEn")
{
    field(ZRST, "disable")
    field(ONST, "enable")
    field(ZRVL, "0")
    field(ONVL, "1")
    field(DTYP, "Raw Soft Channel")
    field(OUT,  "$(P)OutputMode PP")
}
